{
  "name": "mumocast.slides",
  "version": "0.1.0",
  "description": "Create MulmoScript from Keynote and Powerpoint",
  "homepage": "https://github.com/receptron/MulmoCast-Slides#readme",
  "bugs": {
    "url": "https://github.com/receptron/MulmoCast-Slides/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/receptron/MulmoCast-Slides.git"
  },
  "license": "MIT",
  "author": "receptron",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "tsx --test ./tests/test_*.ts",
    "test:keynote": "osascript tools/keynote/extract.scpt samples/GraphAI.key",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "keynote": "osascript tools/keynote/extract.scpt",
    "pptx": "tsx src/convert/pptx.ts",
    "pdf": "tsx src/convert/pdf.ts",
    "marp": "tsx src/convert/marp.ts",
    "test:marp": "tsx src/convert/marp.ts samples/sample.md",
    "test:marp:theme": "tsx src/convert/marp.ts samples/custom_theme_demo.md --theme samples/custom-ocean.css",
    "movie": "tsx src/actions/movie.ts",
    "bundle": "tsx src/actions/bundle.ts",
    "upload": "tsx src/actions/upload.ts"
  },
  "dependencies": {
    "@marp-team/marp-cli": "^4.0.0",
    "dotenv": "^17.2.3",
    "mulmocast": "^2.1.16",
    "node-pptx-parser": "^1.0.1",
    "openai": "^6.15.0",
    "pdf-parse": "^2.4.5",
    "ppt-png": "^2.2.0",
    "tsx": "^4.7.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/yargs": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "unicorn-magic": "0.2.0"
  }
}
